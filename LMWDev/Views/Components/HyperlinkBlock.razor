@using Page_Library.Page.Entities.ContentBlock
@using Microsoft.AspNetCore.Components

@code {
    [Parameter]
    public Page_Library.Page.Entities.ContentBlock.HyperlinkBlock Block { get; set; }
}

<div style="text-align:@Block?.Alignment;">
    <a href="@Block?.Url" target="_blank" rel="noopener noreferrer">
        @Block?.LinkText
    </a>
</div>
