@    using System.Data
@    using UIFactory.Factory.CSHTML.Concrete.Video
@model Video

<div style="text-align: center">
	<h1> @Model.Title </h1>
	<video width="320" height="240" id="@Model.GUID" Name="@Model.GUID" controls>
		<source src="~/Videos/@Model.Source" type="video/mp4">
		Your browser does not support the video tag.
	</video>
	<p>@Model.Description</p>
</div>

